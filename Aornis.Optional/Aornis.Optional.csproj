<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <Authors>Richard Thorne</Authors>
    <Company>Aornis</Company>
    <Description>A productive Optional library for .NET</Description>
    <Copyright>Richard Thorne</Copyright>
    <PackageLicense>https://www.apache.org/licenses/LICENSE-2.0</PackageLicense>
    <PackageProjectUrl>https://github.com/Foritus/optional-dot-net</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Foritus/optional-dot-net.git</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags />
    <PackageId>Optional.net</PackageId>
    <Product>Optional.net</Product>
    <Version>1.2</Version>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <LangVersion>latest</LangVersion>
    <PackageVersion>2.0</PackageVersion>
    <TargetFrameworks>netstandard2.0;netcoreapp3.0;netcoreapp3.1</TargetFrameworks>
  </PropertyGroup>

</Project>
